#pragma once

#include <QtCore/QMimeData>

namespace KUrlMimeData {

enum DecodeOptions {
    PreferLocalUrls,
    PreferKdeUrls,
};

inline bool exportUrlsToPortal(QMimeData *mimeData) {
    return false;
}

inline auto urlsFromMimeData(const QMimeData *mimeData, ...) {
    return mimeData->urls();
}

} // namespace KUrlMimeData
